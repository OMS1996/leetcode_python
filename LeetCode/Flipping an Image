class Solution(object):
    def flipAndInvertImage(self,lst):

        newa=[]
        for i,sub_lst in enumerate(lst):
            sub_lst.reverse()
            for j,value in enumerate(sub_lst):
                if value==0:
                    sub_lst[j]=1
                elif value==1:
                    sub_lst[j]=0
            newa.append(sub_lst)
        
        return newa  
