class Solution(object):
    def countCharacters(self, words, chars):
        """
        :type words: List[str]
        :type chars: str
        :rtype: int
        """
        
        res = 0
        for word in words:
            flag = True
            for i in word:
                if word.count(i) > chars.count(i):
                    flag = False
                    break
            if flag: res += len(word)
        return res
                
